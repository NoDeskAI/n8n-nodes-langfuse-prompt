{
	"name": "@nodeskai/n8n-nodes-langfuse-prompt",
	"version": "0.1.0",
	"description": "n8n community node to fetch and render prompts from Langfuse",
	"license": "MIT",
	"homepage": "https://github.com/NoDeskAI/n8n-nodes-langfuse-prompt",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"langfuse",
		"prompt",
		"ai"
	],
	"author": {
		"name": "xzq-xu",
		"email": "zhiqiang.xu@nodeskai.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/NoDeskAI/n8n-nodes-langfuse-prompt.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
